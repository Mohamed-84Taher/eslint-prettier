{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base","prettier"
    ],
    "plugins": ["prettier"],
  
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
        "prettier/prettier": "error",
        "linebreak-style": 0,
        "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
        "no-console":"off"
        // "eol-last": ["error", "never"]
      }
}
